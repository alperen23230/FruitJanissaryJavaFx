<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.GameMenu">
   <children>
      <Label layoutX="179.0" layoutY="63.0" text="Fruit Janissary" textFill="#da2727">
         <font>
            <Font name="SansSerif Bold" size="36.0" />
         </font>
      </Label>
      <Button layoutX="260.0" layoutY="129.0" mnemonicParsing="false" onAction="#goToGameScene" prefHeight="42.0" prefWidth="94.0" text="New Game">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Button layoutX="260.0" layoutY="231.0" mnemonicParsing="false" onAction="#goToProfile" prefHeight="42.0" prefWidth="94.0" text="Profile">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Button layoutX="260.0" layoutY="285.0" mnemonicParsing="false" onAction="#signOutClicked" prefHeight="42.0" prefWidth="94.0" text="Sign Out">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
      <Label fx:id="usernameLabel" layoutX="39.0" layoutY="28.0" text="Label">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button layoutX="260.0" layoutY="179.0" mnemonicParsing="false" onAction="#goToLeaderboard" prefHeight="42.0" prefWidth="94.0" text="Leaderboard">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
