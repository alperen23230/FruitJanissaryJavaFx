<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.EndGameController">
   <children>
      <Button layoutX="261.0" layoutY="233.0" mnemonicParsing="false" onAction="#playAgain" prefHeight="25.0" prefWidth="77.0" text="Play Again" />
      <Button layoutX="262.0" layoutY="278.0" mnemonicParsing="false" onAction="#goToMainMenu" text="Main Menu" />
      <Label layoutX="248.0" layoutY="125.0" text="Score:">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label fx:id="scoreLbl" layoutX="319.0" layoutY="125.0" text="Label">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label layoutX="220.0" layoutY="152.0" text="Duration:">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label fx:id="durationLbl" layoutX="319.0" layoutY="152.0" text="Label">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Label fx:id="highScoreLbl" layoutX="214.0" layoutY="78.0" text="Label">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
